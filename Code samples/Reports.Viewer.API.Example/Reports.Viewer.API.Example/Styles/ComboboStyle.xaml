                               Margin="1"  >
							<themes:SystemDropShadowChrome x:Uid="themes:SystemDropShadowChrome_2" x:Name="shadow" Color="Transparent" MaxHeight="{TemplateBinding MaxDropDownHeight}" MinWidth="{Binding ActualWidth, ElementName=templateRoot}">
								<Grid x:Uid="DropDown" Name="DropDown" SnapsToDevicePixels="True"  MinWidth="{TemplateBinding ActualWidth}" MaxHeight="{TemplateBinding MaxDropDownHeight}">
									<Border x:Uid="Border_2" x:Name="dropDownBorder" BorderBrush="{DynamicResource {x:Static SystemColors.WindowFrameBrushKey}}" BorderThickness="1" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}">
										<ScrollViewer x:Uid="ScrollViewer_2" Margin="1,1,2,1" SnapsToDevicePixels="True" DataContext="{Binding}">
											<StackPanel x:Uid="StackPanel_1" IsItemsHost="True" KeyboardNavigation.DirectionalNavigation="Contained" />
										</ScrollViewer>
									</Border>
								</Grid>
							</themes:SystemDropShadowChrome>
						</Popup>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger x:Uid="Trigger_17" Property="HasItems" Value="false">
							<Setter x:Uid="Setter_100" TargetName="dropDownBorder" Property="MinHeight" Value="95"/>
						</Trigger>
						<Trigger x:Uid="Trigger_18" Property="IsEnabled" Value="false">
							<Setter x:Uid="Setter_101" Property="Foreground" Value="{StaticResource ComboBoxItem.Disabled.ForegroundBrush}"/>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>


