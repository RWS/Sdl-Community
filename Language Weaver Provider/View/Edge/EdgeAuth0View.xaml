<Window x:Class="LanguageWeaverProvider.View.Edge.EdgeAuth0View"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:viewModel="clr-namespace:LanguageWeaverProvider.ViewModel.Edge"
		xmlns:wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
		WindowStartupLocation="CenterScreen"
		mc:Ignorable="d"
		d:DataContext="{d:DesignInstance {x:Type viewModel:EdgeAuth0ViewModel}}"
		Title="Language Weaver Browser"
		Height="600"
		Width="500">
	<Grid>
		<wpf:WebView2 Name="WebView2Browser"
					  Loaded="WebView2Browser_OnLoaded"/>
	</Grid>
</Window>