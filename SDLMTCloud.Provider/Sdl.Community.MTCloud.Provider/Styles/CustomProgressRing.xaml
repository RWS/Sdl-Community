<ResourceDictionary x:Uid="ResourceDictionary_1"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls1="clr-namespace:Sdl.Community.MTCloud.Provider.Controls">

    <Style TargetType="{x:Type controls1:CustomProgressRing}" x:Key="Sdl.CustomProgressRing.Normal">
        <Setter Property="Text" Value="One moment" />
        <Setter Property="Foreground" Value="#636463" />
        <!--TODO: Use color from palette-->
        <Setter Property="SpinnerColor"
                Value="#FF008080" />
        <Setter Property="SpinnerSize"
                Value="30" />
        <Setter Property="SpinnerThickness"
                Value="5" />
        <Setter Property="Padding"
                Value="5" />
    </Style>
</ResourceDictionary>