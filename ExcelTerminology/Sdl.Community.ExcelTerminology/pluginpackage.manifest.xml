<?xml version="1.0" encoding="utf-8"?>
<PluginPackage xmlns="http://www.sdl.com/Plugins/PluginPackage/1.0">
  <PlugInName>TermExcelerator</PlugInName>
  <Version>2.2.1.0</Version>
  <Description>Excel based terminology provider. It allows to specify the columns from worksheet as source and target terms including synonyms separated by a delimitation.</Description>
  <Author>SDL Community Developers</Author>
  <RequiredProduct name="SDLTradosStudio" minversion="15.0" />
  <Include>
    <File>EPPlus.dll</File>
    <File>Newtonsoft.Json.dll</File>
    <File>ObjectListView.dll</File>
    <File>Microsoft.AI.Agent.Intercept.dll</File>
    <File>Microsoft.AI.DependencyCollector.dll</File>
    <File>Microsoft.AI.PerfCounterCollector.dll</File>
    <File>Microsoft.AI.ServerTelemetryChannel.dll</File>
    <File>Microsoft.AI.WindowsServer.dll</File>
		<File>NLog.dll</File>
	</Include>
</PluginPackage>